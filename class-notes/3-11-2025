//Constructor function (WILL START WITH UPPERCASE, ASSIGN GIVEN PARAMETERS TO (THIS) KEYWORD) ***NOT USED ANYMORE***

function Mug(material, price, designName) {
    this.material = material;
    this.price = price;
    this.designName = designName;
}

const mugJanuary = new Mug('glass', 36,08, 'design1')
const mugFeburary = new Mug('glass', 39,58, 'design2')
const mugMarch = new Mug('glass', 34,38, 'design3')





//CLASS CONCSTRUCTOR *Now USED*
class Student {
    constructer(fullname, age, program) {
        this.fullname = fullname
        this.age = age
        this.program = program
    }
}

const student1 = new Student('John Doe', 50, 'SDET')
const student2 = new Student('Moe Hasjeer', 22, 'SDET')
const student3 = new Student('Mohamad Hishmoe', 25, 'SE')